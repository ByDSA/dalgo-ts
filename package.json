{
  "name": "dalgo",
  "version": "0.0.1",
  "description": "Estructuras y algoritmos comunes de programación en TypeScript",
  "scripts": {
    "prebuild": "pnpm clean:build && pnpm rebuild && pnpm test",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm test:coverage && pnpm lint:fix && pnpm build",
    "postpublish": "pnpm clean",
    "build": "pnpm build:raw",
    "build:raw": "tsc -p tsconfig-build.json",
    "clean:build": "rm -rf build",
    "clean:modules": "rm -rf node_modules",
    "test": "NODE_ENV=test jest --maxWorkers=1",
    "test:watch": "pnpm test -- --watch",
    "test:coverage": "pnpm test -- --coverage",
    "release": "standard-version",
    "prepare": "husky install",
    "lint:eslint": "eslint **/src/*.ts",
    "lint:prettier": "prettier **/src/*.ts",
    "lint:fix": "prettier --write **/src/*.ts && eslint ./src/*.ts --fix"
  },
  "author": "Daniel Sales Álvarez <danisales.es@gmail.com>",
  "license": "GPLv3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ByDSA/dalgo-ts.git"
  },
  "bugs": {
    "url": "https://github.com/ByDSA/dalgo-ts/issues"
  },
  "homepage": "https://github.com/ByDSA/dalgo-ts#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "29.5.0",
    "jest": "29.5.0",
    "jest-expect-message": "1.1.3",
    "ts-jest": "29.1.0",
    "tslib": "2.5.0",
    "typescript": "5.0.4",
    "@commitlint/cli": "17.6.1",
    "@commitlint/config-conventional": "17.6.1",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "eslint": "8.38.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "prettier": "2.8.7",
    "standard-version": "9.5.0",
    "tsconfig-paths": "4.2.0",
    "babel-jest": "29.5.0"
  }
}
